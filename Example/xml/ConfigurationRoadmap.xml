<?xml version="1.0"?>
<root   xmlns="http://polimi.it/GDMImporter"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://polimi.it/GDMImporter /Example/xml/configurationSchema.xsd">
    <settings>
        <!--BASE WORKING FOLDER FOR THE IMPORTER-->
        <base_working_directory>C:\Users\ricca\Desktop\tesi\data</base_working_directory>
        <download_enabled>true</download_enabled>
        <transform_enabled>true</transform_enabled>
        <load_enabled>false</load_enabled>
        <parallel_execution>false</parallel_execution>
    </settings>
    <source_list>
        <source name="Roadmap_Epigenomics">
            <url>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/</url>
            <source_working_directory>RoadmapEpigenomics</source_working_directory>
            <downloader>it.polimi.genomics.importer.RoadmapImporter.RoadmapDownloader</downloader>
            <transformer>it.polimi.genomics.importer.RoadmapImporter.RoadmapTransformer</transformer>
            <loader>it.polimi.genomics.importer.GMQLImporter.GMQLLoader</loader>
            <download_enabled>true</download_enabled>
            <transform_enabled>true</transform_enabled>
            <load_enabled>false</load_enabled>
            <parameter_list>     
                <parameter>
                    <description>to know which user in gmql is going to be imported</description>
                    <key>gmql_user</key>
                    <value>public</value>
                </parameter>
            </parameter_list>
            <dataset_list>
                <dataset name="narrowPeak">
                    <dataset_working_directory>narrowPeak</dataset_working_directory>
                    <schema_url location="local">schemas/narrowPeak.schema</schema_url>
                    <download_enabled>false</download_enabled>
                    <transform_enabled>false</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes peak calls in narrowPeak format</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/narrowPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.narrowPeak.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="broadPeak">
                    <dataset_working_directory>broadPeak</dataset_working_directory>
                    <schema_url location="local">schemas/broadPeak.schema</schema_url>
                    <download_enabled>false</download_enabled>
                    <transform_enabled>false</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes peak calls in broadPeak format</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/broadPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.broadPeak.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="gappedPeak">
                    <dataset_working_directory>gappedPeak</dataset_working_directory>
                    <schema_url location="local">schemas/gappedPeak.schema</schema_url>
                    <download_enabled>false</download_enabled>
                    <transform_enabled>false</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes peak calls in gappedPeak format</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/gappedPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.gappedPeak.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="DNase_narrow_bed">
                    <dataset_working_directory>all_narrow_bed</dataset_working_directory>
                    <schema_url location="local">schemas/narrow_bed.schema</schema_url>
                    <download_enabled>false</download_enabled>
                    <transform_enabled>false</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes DNase narrow peak calls in bed format (no pvalue threshold)</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/narrowPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.all.peaks.bed.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="narrow_fdr_bed">
                    <dataset_working_directory>fdr_narrow_bed</dataset_working_directory>
                    <schema_url location="local">schemas/narrow_bed.schema</schema_url>
                    <download_enabled>true</download_enabled>
                    <transform_enabled>true</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes DNase narrow peak calls in bed format (pvalue threshold set at 0.01)</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/narrowPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.fdr0.01.peaks.bed.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="DNase_broad_bed">
                    <dataset_working_directory>DNase_broad_bed</dataset_working_directory>
                    <schema_url location="local">schemas/broad_bed.schema</schema_url>
                    <download_enabled>false</download_enabled>
                    <transform_enabled>false</transform_enabled>
                    <load_enabled>false</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>loading_description</key>
                            <value>Roadmap Epigenomics conolidated epigenomes DNase broad peak calls in bed format</value>
                        </parameter>
                        <parameter>
                            <key>spreadsheet_url</key>
                            <value>https://docs.google.com/spreadsheets/d/1yikGx4MsO9Ei36b64yOy9Vb6oPC5IBGlFbYEt-N6gOM/edit#gid=15</value>
                        </parameter>
                        <parameter>
                            <key>folder_regex</key>
                            <value>http://egg2.wustl.edu/roadmap/data/byFileType/peaks/consolidated/broadPeak/</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*\.bed.gz</value>
                        </parameter>
                    </parameter_list>
                </dataset>
            </dataset_list>
        </source>
    </source_list>
</root>